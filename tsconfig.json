{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./bin",
    // "rootDir": "src",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/routes/*": ["src/routes/*"],
      "@/config": ["src/config/index"],
      "@/loaders": ["src/loaders/index"],
      "@/controllers": ["src/controllers/*"]
    }
  },
  // "include": ["src/**/*"],
  "exclude": ["node_modules"]
}